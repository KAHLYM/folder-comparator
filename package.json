{
  "name": "folder-comparator",
  "displayName": "Folder Comparator",
  "description": "No description provided",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "folder-comparator.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "folder-comparator.selectForCompare",
        "title": "Select Folder for Compare"
      },
      {
        "command": "folder-comparator.compareWithSelected",
        "title": "Compare Folder with Selected"
      }
    ],
    "menus": {
      "explorer/context": [{
        "command": "folder-comparator.selectForCompare",
        "group": "context@1",
        "when": "explorerResourceIsFolder"
      },
      {
        "command": "folder-comparator.compareWithSelected",
        "group": "context@2",
        "when": "explorerResourceIsFolder && folder-comparator.showCompareWithSelected"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
